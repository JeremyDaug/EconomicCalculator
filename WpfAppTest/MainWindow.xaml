<Window x:Class="WpfAppTest.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfAppTest"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Label Content="Economic Simulator" HorizontalAlignment="Center" 
               Margin="0,15,0,0" VerticalAlignment="Top"/>
        <Label Content="Alpha Version 0.1" HorizontalAlignment="Center"
               VerticalAlignment="Top" Margin="0,30,0,0" TextElement.Foreground="Gray"/>
        <Button Name="LoadTestDataBtn" ClickMode="Release"
            Content="Load Test Data" HorizontalAlignment="Center"
                VerticalAlignment="Center" Width="100" />

    </Grid>
</Window>
