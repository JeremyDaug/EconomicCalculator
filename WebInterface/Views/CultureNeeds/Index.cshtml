@model IEnumerable<EconModels.PopulationModel.CultureNeed>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CultureId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Need)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Amount)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NeedType)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Culture.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Need.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Amount)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NeedType)
        </td>
        <td>
            @Html.ActionLink("Details", "Details", new { id = item.CultureId, needId = item.NeedId }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.CultureId, needId = item.NeedId })
        </td>
    </tr>
}

</table>
