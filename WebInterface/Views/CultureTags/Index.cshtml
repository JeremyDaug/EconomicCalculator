@model IEnumerable<EconModels.PopulationModel.CultureTag>

@{
    ViewBag.Title = "Index";
}

<h2>Culture Tags</h2>

<p>
    @Html.ActionLink("Create New", "Create") |
    @Html.ActionLink("Back to Cultures", "Index", "Cultures", null, null)
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Culture.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Tag)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Culture.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItm => item.Tag)
        </td>
        <td>
            @Html.ActionLink("Details", "Details", new { cultureId = item.CultureId, tag = item.Tag }) |
            @Html.ActionLink("Delete", "Delete", new { cultureId = item.CultureId, tag = item.Tag })
        </td>
    </tr>
}

</table>
<p>
    Editing Is unavailable. Delete the old and make a new instead.
</p>